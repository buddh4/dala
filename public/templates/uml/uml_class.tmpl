<g>
    <rect id="node_back_{node_id}" width="{width}" height="53" rx="2" style="fill:{fill};stroke:{stroke};stroke-dasharray:{stroke-dasharray};stroke-width:{stroke-width}"/>
    <text id="title_t_{node_id}" y="5" font-family="Helvetica" font-size="11" text-anchor="start" dominant-baseline="hanging">
        <tspan id="title_span_{node_id}" dominant-baseline="inherit">{title}</tspan>
    </text>
    <g id="attributes_g_{node_id}" transform="translate(0 20)">
        <rect id="attributes_back_{node_id}" width="{width}" height="17" fill="white" style="stroke:#000000;fill:{color_second};" stroke-width="1"/>
        <text id="attributes_t_{node_id}" font-family="Helvetica" font-size="11" text-anchor="start" transform="translate(4 4)" dominant-baseline="hanging">
            <tspan id="attributes_span_{node_id}" dominant-baseline="inherit">attributes</tspan>
        </text>
    </g>
    <g id="methods_g_{node_id}" transform="translate(0 39)">
        <rect id="methods_back_{node_id}" width="{width}" height="17" fill="white" style="stroke:#000000;fill:{color_second};" stroke-width="1"/>
        <text id="methods_t_{node_id}" font-family="Helvetica" font-size="11" text-anchor="start" transform="translate(4 4)" dominant-baseline="hanging">
            <tspan id="methods_span_{node_id}"  dominant-baseline="inherit">operations</tspan>
        </text>
    </g>
</g>